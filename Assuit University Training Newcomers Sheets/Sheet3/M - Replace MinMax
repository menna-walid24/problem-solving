#include<iostream>
using namespace std;
#include <bits/stdc++.h>
#define int long long
#define all(v) v.begin(), v.end()
void solve(){
 int n;
cin>>n;
vector<int>v(n);
 for(int i=0;i<n;i++){
  cin>>v[i];  }
int mn=*min_element(all(v));
int mx=*max_element(all(v));
for(int i=0;i<n;i++){
    if(v[i]==mn){
        v[i]=mx;
    }
   else if(v[i]==mx){
    v[i]=mn;
   }
}
for(auto c:v){
    cout<<c<<' ';
}
}
signed main() {
  
 solve();
    return 0;
}
