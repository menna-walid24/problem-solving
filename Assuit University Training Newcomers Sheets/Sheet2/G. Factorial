#include <iostream>

using namespace std;

int main()
{
   int n;
   cin>>n;
   int arr[n];


   for(int i=0;i<n;i++){
        cin>>arr[i];
        }
        for(int i=0;i<n;i++){
                long long m=1;
            for(int j=arr[i];j>=1;j--){
            m*=j;
        }
        cout<<m<<endl;
        }







    return 0;
}
