#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    long long A,B,C,max,min;
long long  arr[3];




  for(int i=0;i<3;i++){
    cin>>arr[i];
}
max=arr[0];
   min=arr[0];
for(int i=0;i<3;i++){

  if(arr[i]>max){
    max=arr[i];
  }
}
  for (int i=0;i<3;i++){
  if(arr[i]<min){
    min=arr[i];
  }
  }
  cout<<min<<" "<<max;
    return 0;
}
