#include <iostream>
#include <algorithm>
using namespace std;
 void dis(int a, int b,int c){

    cout<<a<<endl<<b<<endl<<c<<endl;

}


int main()
{
  int n=3,a,b,c;

cin>>a>>b>>c;

int arr[3]={a,b,c};

 sort(arr,arr+n);
 for(int i=0;i<3;i++){
    cout<<arr[i]<<endl;
 }



cout<<endl;
dis(a,b,c);

return 0;
}
